<?xml version="1.0" encoding="UTF-8"?>
<plug-in>
    <name>kibana</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>kibana.plugin.description</description>
    <provider>kibana.plugin.provider</provider>
    <provider-url>http://dev.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/kibana/kibana.png</icon-url>
    <copyright>Copyright (c) 2014 City of Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>KIBANA_MANAGEMENT</feature-id>
            <feature-title>kibana.adminFeature.KibanaDashboard.name</feature-title>
            <feature-description>kibana.adminFeature.KibanaDashboard.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/kibana/KibanaDashboard.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
        </admin-feature>
    </admin-features>    

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>kibana</application-id>
            <application-class>fr.paris.lutece.plugins.kibana.web.KibanaApp</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
    </portlets>
</plug-in>